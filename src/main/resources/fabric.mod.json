{
  "schemaVersion": 1,
  "id": "smoothbeta",
  "version": "${version}",

  "name": "SmoothBeta",
  "description": "Additional settings for beta that allow for smoother experience!",
  "authors": [
    "mine_diver"
  ],
  "contact": {
    "homepage": "https://glass-repo.net/repo/mod/smoothbeta",
    "sources": "https://github.com/mineLdiver/smoothbeta",
    "issues": "https://github.com/mineLdiver/smoothbeta/issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/smoothbeta/icon.png",

  "environment": "client",
  "entrypoints": {
    "stationapi:event_bus_client": [
      "net.mine_diver.smoothbeta.SmoothBeta",
      "net.mine_diver.smoothbeta.level.LevelListener"
    ]
  },
  "mixins": [
    {
      "environment": "client",
      "config": "vsync.mixins.json"
    }
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3",
    "stationapi": ">=2.0-PRE2"
  },

  "custom": {
    "modmenu:clientsideOnly": true
  }
}
